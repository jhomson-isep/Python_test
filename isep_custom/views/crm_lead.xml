      <xpath expr="//field[@name='fax']" position="attributes">
        <attribute name="invisible">True</attribute>
      </xpath>


      <xpath expr="//field[@name='opt_out']" position="attributes">
        <attribute name="invisible">True</attribute>
      </xpath>
      -->



