<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <!-- OPORTUNIDADES -->


  <record id="isep_crm_case_form_view_oppor" model="ir.ui.view">
    <field name="model">crm.lead</field>
    <field name="inherit_id" ref="crm.crm_case_form_view_oppor"/>
    <field name="arch" type="xml">

      <xpath expr="//field[@name='partner_id']" position="replace">
        <field name="partner_id" on_change="on_change_partner_id(partner_id)" string="Cliente" context="{'x_producto_id':x_producto_id,'default_name': partner_name, 'default_street': street,                                          'default_street2': street2, 'default_city': city,                                          'default_state_id': state_id, 'default_zip': zip,                                          'default_country_id': country_id, 'default_function': function,                                          'default_phone': phone, 'default_mobile': mobile,                                          'default_email': email_from,                                          'default_user_id': user_id, 'default_team_id': team_id}"/>
      </xpath>

      <xpath expr="//field[@name='email_from']" position="replace">
        <field name="email_from" string="Email" widget="email" context="{'x_producto_id':x_producto_id,'partner_id':partner_id,'street':street,'street2':street2,'zip':zip,'city':city,'state_id':state_id, 'country_id':country_id,'title':title,'email_from':email_from,'phone':phone,'mobile':mobile, 'user_id':user_id,'x_sexo':x_sexo, 'team_id':team_id, 'x_universidad':x_universidad,'x_horario_id':x_horario_id, 'x_titulacion':x_titulacion,'contact_name':contact_name, 'x_aceptacondiciones':x_aceptacondiciones,'x_tipodedocumento':x_tipodedocumento, 'x_documentodeidentidad':x_documentodeidentidad, 'x_finalizacionestudios':x_finalizacionestudios, 'x_annonacimiento':x_annonacimiento, 'x_profesion':x_profesion, 'x_tipodecontacto':x_tipodecontacto}" on_change="on_change_partner_fields(partner_id,context)"/>
      </xpath>

      <xpath expr="//field[@name='phone']" position="replace">
        <field name="phone" widget="phone" context="{'x_producto_id':x_producto_id,'partner_id':partner_id,'street':street,'street2':street2,'zip':zip,'city':city,'state_id':state_id, 'country_id':country_id,'title':title,'email_from':email_from,'phone':phone,'mobile':mobile, 'user_id':user_id,'x_sexo':x_sexo, 'team_id':team_id, 'x_universidad':x_universidad,'x_horario_id':x_horario_id, 'x_titulacion':x_titulacion,'contact_name':contact_name, 'x_aceptacondiciones':x_aceptacondiciones,'x_tipodedocumento':x_tipodedocumento, 'x_documentodeidentidad':x_documentodeidentidad, 'x_finalizacionestudios':x_finalizacionestudios, 'x_annonacimiento':x_annonacimiento, 'x_profesion':x_profesion, 'x_tipodecontacto':x_tipodecontacto}"/>
      </xpath>

      <!--
      <xpath expr="//label[@for='next_activity_id']" position="attributes">
        <attribute name="invisible">True</attribute>
      </xpath>

      <xpath expr="//field[@name='next_activity_id']" position="attributes">
        <attribute name="invisible">True</attribute>
      </xpath>
      -->

      <xpath expr="//notebook[1]" position="before">
        <group string="Información Personalizada">
            <group>
                <!--
                <field name="x_tipodecurso_id"/>

              -->
                <field name="x_curso_id" context="{'display_default_code':True}"/>
                <field name="x_sede_id" string="Delegación"/>
                <field name="x_producto_id" on_change="on_change_prod(x_producto_id)" context="{'partner_id':partner_id,'x_producto_id': x_producto_id}"/>
                <field name="x_oldcrmpotentialid" groups="base.group_system"/>
                <field name="company_id" groups="base.group_system"/>
            </group>

            <group>
                <field name="x_area_id"/>
                <field name="x_modalidad_id"/>
            </group>
        </group>
      </xpath>

      <xpath expr="//page[1]" position="replace">
        <page string="Descripción">
          <field name="description"/>
        </page>
      </xpath>

      <xpath expr="//field[@name='partner_name']" position="attributes">
        <attribute name="invisible">True</attribute>
      </xpath>

      <xpath expr="//field[@name='street']" position="replace">
        <field name="street" placeholder="Calle..." class="o_address_street" context="{'x_producto_id':x_producto_id,'partner_id':partner_id,'street':street,'street2':street2,'zip':zip,'city':city,'state_id':state_id, 'country_id':country_id,'title':title,'email_from':email_from,'phone':phone,'mobile':mobile, 'user_id':user_id,'x_sexo':x_sexo, 'team_id':team_id, 'x_universidad':x_universidad,'x_horario_id':x_horario_id, 'x_titulacion':x_titulacion,'contact_name':contact_name, 'x_aceptacondiciones':x_aceptacondiciones,'x_tipodedocumento':x_tipodedocumento, 'x_documentodeidentidad':x_documentodeidentidad, 'x_finalizacionestudios':x_finalizacionestudios, 'x_annonacimiento':x_annonacimiento, 'x_profesion':x_profesion, 'x_tipodecontacto':x_tipodecontacto}"/>
      </xpath>

      <xpath expr="//field[@name='street2']" position="replace">
        <field name="street2" placeholder="Calle 2..." class="o_address_street" context="{'x_producto_id':x_producto_id,'partner_id':partner_id,'street':street,'street2':street2,'zip':zip,'city':city,'state_id':state_id, 'country_id':country_id,'title':title,'email_from':email_from,'phone':phone,'mobile':mobile, 'user_id':user_id,'x_sexo':x_sexo, 'team_id':team_id, 'x_universidad':x_universidad,'x_horario_id':x_horario_id, 'x_titulacion':x_titulacion,'contact_name':contact_name, 'x_aceptacondiciones':x_aceptacondiciones,'x_tipodedocumento':x_tipodedocumento, 'x_documentodeidentidad':x_documentodeidentidad, 'x_finalizacionestudios':x_finalizacionestudios, 'x_annonacimiento':x_annonacimiento, 'x_profesion':x_profesion, 'x_tipodecontacto':x_tipodecontacto}"/>
      </xpath>

      <xpath expr="//field[@name='city']" position="replace">
        <field name="city" placeholder="Ciudad" class="o_address_city" context="{'x_producto_id':x_producto_id,'partner_id':partner_id,'street':street,'street2':street2,'zip':zip,'city':city,'state_id':state_id, 'country_id':country_id,'title':title,'email_from':email_from,'phone':phone,'mobile':mobile, 'user_id':user_id,'x_sexo':x_sexo, 'team_id':team_id, 'x_universidad':x_universidad,'x_horario_id':x_horario_id, 'x_titulacion':x_titulacion,'contact_name':contact_name, 'x_aceptacondiciones':x_aceptacondiciones,'x_tipodedocumento':x_tipodedocumento, 'x_documentodeidentidad':x_documentodeidentidad, 'x_finalizacionestudios':x_finalizacionestudios, 'x_annonacimiento':x_annonacimiento, 'x_profesion':x_profesion, 'x_tipodecontacto':x_tipodecontacto}"/>
      </xpath>

      <xpath expr="//field[@name='zip']" position="replace">
