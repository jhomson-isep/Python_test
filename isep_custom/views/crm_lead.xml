      </xpath>

      <xpath expr="//field[@name='title']" position="replace">
        <field name="title" placeholder="Título" domain="[]" options="{&quot;no_open&quot;: True}" context="{'x_producto_id':x_producto_id,'partner_id':partner_id,'street':street,'street2':street2,'zip':zip,'city':city,'state_id':state_id, 'country_id':country_id,'title':title,'email_from':email_from,'phone':phone,'mobile':mobile, 'user_id':user_id,'x_sexo':x_sexo, 'team_id':team_id, 'x_universidad':x_universidad,'x_horario_id':x_horario_id, 'x_titulacion':x_titulacion,'contact_name':contact_name, 'x_aceptacondiciones':x_aceptacondiciones,'x_tipodedocumento':x_tipodedocumento, 'x_documentodeidentidad':x_documentodeidentidad, 'x_finalizacionestudios':x_finalizacionestudios, 'x_annonacimiento':x_annonacimiento, 'x_profesion':x_profesion, 'x_tipodecontacto':x_tipodecontacto}" on_change="on_change_partner_fields(partner_id,context)"/>
      </xpath>

      <xpath expr="//field[@name='function']" position="attributes">
        <attribute name="invisible">True</attribute>
      </xpath>

      <xpath expr="//field[@name='mobile']" position="replace">
        <field name="mobile" context="{'x_producto_id':x_producto_id,'partner_id':partner_id,'street':street,'street2':street2,'zip':zip,'city':city,'state_id':state_id, 'country_id':country_id,'title':title,'email_from':email_from,'phone':phone,'mobile':mobile, 'user_id':user_id,'x_sexo':x_sexo, 'team_id':team_id, 'x_universidad':x_universidad,'x_horario_id':x_horario_id, 'x_titulacion':x_titulacion,'contact_name':contact_name, 'x_aceptacondiciones':x_aceptacondiciones,'x_tipodedocumento':x_tipodedocumento, 'x_documentodeidentidad':x_documentodeidentidad, 'x_finalizacionestudios':x_finalizacionestudios, 'x_annonacimiento':x_annonacimiento, 'x_profesion':x_profesion, 'x_tipodecontacto':x_tipodecontacto}"/>
