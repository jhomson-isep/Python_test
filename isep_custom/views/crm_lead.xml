      </xpath>

      <xpath expr="//page[@name='lead']/group/group/div" position="after">
        <field name="x_aceptacondiciones" context="{'x_producto_id':x_producto_id,'partner_id':partner_id,'street':street,'street2':street2,'zip':zip,'city':city,'state_id':state_id, 'country_id':country_id,'title':title,'email_from':email_from,'phone':phone,'mobile':mobile, 'user_id':user_id,'x_sexo':x_sexo, 'team_id':team_id, 'x_universidad':x_universidad,'x_horario_id':x_horario_id, 'x_titulacion':x_titulacion,'contact_name':contact_name, 'x_aceptacondiciones':x_aceptacondiciones,'x_tipodedocumento':x_tipodedocumento, 'x_documentodeidentidad':x_documentodeidentidad, 'x_finalizacionestudios':x_finalizacionestudios, 'x_annonacimiento':x_annonacimiento, 'x_profesion':x_profesion, 'x_tipodecontacto':x_tipodecontacto}" invisible="1"/>
        <field name="x_tipodecontacto" context="{'x_producto_id':x_producto_id,'partner_id':partner_id,'street':street,'street2':street2,'zip':zip,'city':city,'state_id':state_id, 'country_id':country_id,'title':title,'email_from':email_from,'phone':phone,'mobile':mobile, 'user_id':user_id,'x_sexo':x_sexo, 'team_id':team_id, 'x_universidad':x_universidad,'x_horario_id':x_horario_id, 'x_titulacion':x_titulacion,'contact_name':contact_name, 'x_aceptacondiciones':x_aceptacondiciones,'x_tipodedocumento':x_tipodedocumento, 'x_documentodeidentidad':x_documentodeidentidad, 'x_finalizacionestudios':x_finalizacionestudios, 'x_annonacimiento':x_annonacimiento, 'x_profesion':x_profesion, 'x_tipodecontacto':x_tipodecontacto}"/>

