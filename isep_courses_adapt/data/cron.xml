<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="delete_all_void_records" forcecreate='True' model="ir.cron">
            <field name="name">Delete All Void Records</field>
            <field eval="True" name="active"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_op_gdrive_documents"/>
            <field name="state">code</field>
            <field name="code">model.delete_void_records()</field>
            <field name="type">ir.actions.server</field>
        </record>

        <record id="student_mail_access" forcecreate='True' model="ir.cron">
            <field name="name">Estudiante: Mail segun dias de acceso</field>
            <field eval="True" name="active"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_op_student"/>
            <field name="state">code</field>
            <field name="code">model.cron_send_email()</field>
            <field name="type">ir.actions.server</field>
        </record>

        <record id="add_date_finish_in_admission" forcecreate='True' model="ir.cron">
            <field name="name">Admission: Agregar fecha de cierre de expediente acad√©mico</field>
            <field eval="True" name="active"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_op_admission"/>
            <field name="state">code</field>
            <field name="code">model.date_finish()</field>
            <field name="type">ir.actions.server</field>
        </record>

        <record id="get_grads_from_moodle" forcecreate='True' model="ir.cron">
            <field name="name">OP: Obtener calificaciones desde Moodle</field>
            <field eval="True" name="active"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_op_exam_attendees"/>
            <field name="state">code</field>
            <field name="code">model.import_grades_from_moodle()</field>
            <field name="type">ir.actions.server</field>
        </record>

    </data>
</odoo>