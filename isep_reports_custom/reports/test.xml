<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <report id="test_sale_order"
                model="sale.order"
                string="Test sale order"
                report_type="qweb-pdf"
                name="isep_reports_custom.test_sale_order_report_view"
                menu="True"/>

        <template id="test_sale_order_report_view">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <div class="page">
                        <div class="oe_structure"/>
                        <div class="row">
                            <h2>Sale Order</h2>
                            <p>This object's name is <span
                                    t-field="doc.name"/></p>
                        </div>
                    </div>
                </t>
            </t>
        </template>
    </data>
</odoo>