<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="view_practice_phase_tree" model="ir.ui.view">
            <field name="name">practice.phase.tree</field>
            <field name="model">practice.phase</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Phase">
                    <field name="code"/>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="view_practice_phase_form" model="ir.ui.view">
            <field name="name">practice.phase.form</field>
            <field name="model">practice.phase</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Phase">
                    <sheet>
                        <group col="4">
                            <field name="code"/>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_practice_phase_search" model="ir.ui.view">
            <field name="name">practice.phase.search</field>
            <field name="model">practice.phase</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <search string="Phase">
                    <field name="name"/>
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_open_practice_phase_view">
            <field name="name">Phase</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">practice.phase</field>
            <field name="view_type">form</field>
            <field name="view_mode">form,tree</field>
            <field name="search_view_id" ref="view_practice_phase_search"/>
        </record>

        <record model="ir.actions.act_window.view" id="act_open_practice_phase_form">
            <field name="act_window_id" ref="act_open_practice_phase_view"/>
            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_practice_phase_form"/>
        </record>

        <record model="ir.actions.act_window.view" id="act_open_practice_phase_tree">
            <field name="act_window_id" ref="act_open_practice_phase_view"/>
            <field name="sequence" eval="10"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_practice_phase_tree"/>
        </record>

    </data>
</odoo>