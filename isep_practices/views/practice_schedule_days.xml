<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="view_practice_schedule_days_tree" model="ir.ui.view">
            <field name="name">practice.schedule_days.tree</field>
            <field name="model">practice.schedule.days</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Schedule Days">
                    <field name="day"/>
                    <field name="start_time"/>
                    <field name="end_time"/>
                </tree>
            </field>
        </record>

        <record id="view_practice_schedule_days_form" model="ir.ui.view">
            <field name="name">practice.schedule_days.form</field>
            <field name="model">practice.schedule.days</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Schedule Days">
                    <sheet>
                        <group col="4">
                            <field name="day" required="1"/>
                            <field name="start_time" required="1"/>
                            <field name="end_time" required="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_practice_schedule_days_search" model="ir.ui.view">
            <field name="name">practice.schedule_days.search</field>
            <field name="model">practice.schedule.days</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <search string="Schedule Days">
                    <field name="day"/>
                    <field name="start_time"/>
                    <field name="end_time"/>
                </search>
            </field>
        </record>

    </data>
</odoo>